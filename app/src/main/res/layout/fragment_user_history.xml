<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/text_path" />

            <TextView
                android:id="@+id/userHistory_textView_requested"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yoon_gothic_150"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:text="신청됨"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/userHistory_linear_requested"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="16dp"
                android:orientation="vertical">

            </LinearLayout>

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/text_path" />

            <TextView
                android:id="@+id/userHistory_textView_used"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yoon_gothic_150"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:text="사용중"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/userHistory_linear_used"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="16dp"
                android:orientation="vertical"></LinearLayout>

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/text_path" />

            <TextView
                android:id="@+id/userHistory_textView_delayed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yoon_gothic_150"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:text="연체됨"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/userHistory_linear_delayed"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="16dp"
                android:orientation="vertical"></LinearLayout>

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/text_path" />

            <TextView
                android:id="@+id/userHistory_textView_returned"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yoon_gothic_150"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:text="반납됨"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/userHistory_linear_returned"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="16dp"
                android:orientation="vertical"></LinearLayout>

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/text_path" />

            <TextView
                android:id="@+id/userHistory_textView_expired"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/yoon_gothic_150"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:text="취소됨"
                android:textColor="@color/colorBlack"
                android:textSize="24sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/userHistory_linear_expired"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="16dp"
                android:orientation="vertical"></LinearLayout>

        </LinearLayout>
    </ScrollView>

</LinearLayout>